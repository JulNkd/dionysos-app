<%= simple_form_for(event) do |f| %>
    <%= f.input :title %>
    <%= f.input :category %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :photos, as: :file, input_html: { multiple: true }%>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.submit %>
  <% end %>
